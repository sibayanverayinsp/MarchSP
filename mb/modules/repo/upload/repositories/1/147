#include<stdio.h>

main(){
	int i, j, size;
	
	printf("Enter size of table: ");
	scanf("%d",&size);
	
	for(i=0;i<=size+1;i++){
		for(j=0;j<=size+1;j++){
			if(i == 0){
				if(j == 0)
					printf("x\t");
				else
					printf("%i\t",j-1);
			}
			else{
				if(j == 0)
					printf("%i\t",i-1);
				else
					printf("%i\t",(i-1)*(j-1));
			}
		}
		printf("\n");
	}
}